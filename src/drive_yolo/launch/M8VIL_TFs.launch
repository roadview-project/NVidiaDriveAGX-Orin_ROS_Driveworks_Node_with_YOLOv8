<launch>

	<!-- Car Settings -->
	
	<!-- Car rear Axle -->
	<node pkg="tf2_ros" type="static_transform_publisher" name="CarrearAxle" args="0 0 0.342 0 0 0 car_topographical_zero car_rear_axle" />
	
	<!-- Car Wheels -->
	<node pkg="tf2_ros" type="static_transform_publisher" name="WheelFR" args="2.825 -0.879 0 0 0 0 car_rear_axle Wheel_FR" />
	<node pkg="tf2_ros" type="static_transform_publisher" name="WheelBR" args="0 -0.891 0 0 0 0 car_rear_axle Wheel_BR" />
	<node pkg="tf2_ros" type="static_transform_publisher" name="WheelBL" args="0 0.891 0 0 0 0 car_rear_axle Wheel_BL" />
	<node pkg="tf2_ros" type="static_transform_publisher" name="WheelFL" args="2.830 0.879 0 0 0 0 car_rear_axle Wheel_FL" />

	<!-- Car Sensors Settings -->
